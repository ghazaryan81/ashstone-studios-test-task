  "scripts": {
    "start": "npx concurrently \"npm run dev\" \"npm run sass:watch\"",
    "dev": "npx concurrently \"cross-env NODE_ENV=development npx webpack-dev-server --stats-error-details true --config webpack.config.babel.js\" \"npm run remove\"",
    "prod": "npx concurrently \"cross-env NODE_ENV=production PORT=4601 npx webpack --config webpack.config.babel.js\" \"npm run remove\" \"npm run brows\"",
    "build": "npx concurrently \"npm run prod\"",
    "brows": "npx http-server build",
    "remove": "npx rimraf build",
    "sass:watch": "npx concurrently \"node ./node_modules/sass/sass.js --watch ./src/styles/index.scss ./src/styles/index.css\""
  },
